            <Function name="return" value="2" result-as="scale"/>
            <Condition name="not_equals">
                <Function name="return" value="A" result-as="x"/>
                <Function name="return" value="A" result-as="y"/>
            </Condition>
        </Function>
    </Label>

    <Label group="ROUND_UP">
        <Function name="round_up">
            <Function name="return" value="12.27" result-as="value"/>
            <Function name="return" value="2" result-as="scale"/>
            <Condition name="not_equals">
                <Function name="return" value="A" result-as="x"/>
                <Function name="return" value="A" result-as="y"/>
            </Condition>
        </Function>
    </Label>

    <Label group="SUBSTRING">
        <Function name="substring">
            <Function name="return" value="Hello" result-as="text"/>
            <Function name="return" value="2" result-as="position"/>
            <Condition name="not_equals">
                <Function name="return" value="A" result-as="x"/>
                <Function name="return" value="A" result-as="y"/>
            </Condition>
        </Function>
    </Label>

    <Label group="SUBTRACTION">
        <Function name="subtract">
            <Function name="return" value="5" result-as="number"/>
            <Function name="return" value="2" result-as="subtrahend"/>
            <Condition name="not_equals">
                <Function name="return" value="A" result-as="x"/>
                <Function name="return" value="A" result-as="y"/>
