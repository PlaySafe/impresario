    <Label group="UPPER">
        <Function name="upper">
            <Function name="return" value="Hello"/>
            <Condition name="equals">
                <Function name="return" value="A" result-as="x"/>
                <Function name="return" value="B" result-as="y"/>
            </Condition>
        </Function>
    </Label>
</Labels>
